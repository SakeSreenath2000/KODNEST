<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Achieving AutoWiring using byType  -->
	<bean class="com.kodnest.SpringFramework.Programming.Spring_AutoWiring_Using_byType.Car" id="cr" autowire="byType">

	</bean>
	<!-- Constructor Injection -->
	<bean class="com.kodnest.SpringFramework.Programming.Spring_AutoWiring_Using_byType.Engine" id="eng1">
		<constructor-arg value="5678"></constructor-arg>
		<constructor-arg value="Diesel"></constructor-arg>
	</bean>

</beans>