<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Achieving AutoWiring using byName  -->
	<bean class="com.kodnest.SpringFramework.Programming.Spring_ProfessorApp_AutoWiring_Using_byName.University" id="ur" autowire="byName">

	</bean>
	<!-- Setter Injection -->
	<bean class="com.kodnest.SpringFramework.Programming.Spring_ProfessorApp_AutoWiring_Using_byName.Professor" id="prof">
		<property name="PrId" value="13"></property>
		<property name="PrName" value="Amit"></property>
		<property name="PrSubject" value="Maths"></property>
	</bean>
	<bean class="com.kodnest.SpringFramework.Programming.Spring_ProfessorApp_AutoWiring_Using_byName.Professor" id="prof1">
		<property name="PrId" value="14"></property>
		<property name="PrName" value="Shubam"></property>
		<property name="PrSubject" value="Physics"></property>
	</bean>
	<bean class="com.kodnest.SpringFramework.Programming.Spring_ProfessorApp_AutoWiring_Using_byName.Professor" id="prof2">
		<property name="PrId" value="15"></property>
		<property name="PrName" value="Abishek"></property>
		<property name="PrSubject" value="Chemistry"></property>
	</bean>

</beans>