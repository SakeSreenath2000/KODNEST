<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.kodnest.SpringFramework.Programming.SpringFramework_Wiring.CompanyXYZ" id="cmp">
		<property name="e" ref="emp2"></property>
	</bean>

	<bean class="com.kodnest.SpringFramework.Programming.SpringFramework_Wiring.Employee" id="emp1">
		<property name="empId" value="13"></property>
		<property name="empName" value="Sumit"></property>
		<property name="empSalary" value="55000"></property>
	</bean>
	
	<bean class="com.kodnest.SpringFramework.Programming.SpringFramework_Wiring.Employee" id="emp2">
		<property name="empId" value="19"></property>
		<property name="empName" value="Sourav"></property>
		<property name="empSalary" value="72000"></property>
	</bean>
	
</beans>	